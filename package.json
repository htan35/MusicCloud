{
  "name": "musiccloud",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others-on-fail --prefix-colors \"cyan,magenta\" --names \"API ,Web\" \"npm run server\" \"npm run client\"",
    "dev": "concurrently --kill-others-on-fail --prefix-colors \"cyan,magenta\" --names \"API ,Web\" \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm install && npm run build"
  },
  "dependencies": {
    "assemblyai": "^4.23.1",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.2.0",
    "cloudinary": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}